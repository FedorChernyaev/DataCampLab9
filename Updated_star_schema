ALTER TABLE SAL_DATA.SALE_FACT_TABLE
   DROP CONSTRAINT FK_SALE_FACT_TABLE_REF_DIM_EMPLOYEE_SCD;

ALTER TABLE SAL_DATA.SALE_FACT_TABLE
   DROP CONSTRAINT FK_SALE_FACT_TABLE_REF_DIM_DATE;

ALTER TABLE SAL_DATA.SALE_FACT_TABLE
   DROP CONSTRAINT FK_SALE_FACT_TABLE_REF_DIM_LOCATION;
   
   ALTER TABLE SAL_DATA.SALE_FACT_TABLE
   DROP CONSTRAINT FK_SALE_FACT_TABLE_REF_DIM_CUSTOMER;
   
   ALTER TABLE SAL_DATA.SALE_FACT_TABLE
   DROP CONSTRAINT FK_SALE_FACT_TABLE_REF_DIM_GEN_PERIODS;
   
   ALTER TABLE SAL_DATA.SALE_FACT_TABLE
   DROP CONSTRAINT FK_SALE_FACT_TABLE_REF_DIM_BIKE_SCD;
   
   drop table SAL_DATA.SALE_FACT_TABLE;
   
   
create table SAL_DATA.DIM_EMPLOYEE_SCD (
EMPLOYEE_ID number,
EMPLOYEE_DESC char(50),
EMPLOYEE_FIRST_NAME char(20),
EMPLOYEE_LAST_NAME char(20),
VALID_FROM date,
VALID_TO date,
IS_ACTIVE number,
INSERT_DT date,
constraint PK_DW_EMPLOYEE_SCD primary key (EMPLOYEE_ID) );


create table SAL_DATA.DIM_DATE (
DATE_ID number,
DATE_DESC char(50),
DAY_NUMBER_OF_WEEK number,
DAY_NAME_OF_WEEK char(30),
DAY_NUMBER_OF_MONTH number,
DAY_NUMBER_OF_QUARTER number,
DAY_NUMBER_OF_YEAR number,
IS_WEEKEND number,
IS_HOLYDAY number,
WEEK_NUMBER_OF_MONTH number,
WEEK_NUMBER_OF_QUARTER number,
WEEK_NUMBER_OF_YEAR number,
MONTH_NUMBER_OF_YEAR number,
MONTH_NAME_OF_YEAR char(20),
CALENDAR_QUARTER number,
CALENDAR_YEAR number,
FISCAL_QUARTER number,
FISCAL_YEAR number,
INSERT_DT date,
UPDATE_DT date,
constraint PK_DW_DATE primary key (DATE_ID) );


create table SAL_DATA.DIM_LOCATION (
LOCATION_ID number,
LOCATION_DESC char(50),
ADRESS char(20),
CITY_CODE number,
CITY_NAME char(20),
STATE_CODE number,
STATE_NAME char(20),
COUNTRY_CODE number,
COUNTRY_NAME char(20),
REGION_CODE number,
REGION_NAME char(20),
INSERT_DT date,
UPDATE_DT date,
constraint PK_DW_LOCATION primary key (LOCATION_ID) );


create table SAL_DATA.DIM_CUSTOMER (
CUSTOMER_ID number,
CUSTOMER_DESC char(50),
CUSTOMER_FIRST_NAME char(20),
CUSTOMER_LAST_NAME char(20),
CUSTOMER_AGE number,
CUSTOMER_GENDER char(20),
INSERT_DT date,
UPDATE_DT date,
constraint PK_DW_CUSTOMER primary key (CUSTOMER_ID) );



create table SAL_DATA.DIM_GEN_PERIODS (
PERIOD_ID number,
PERIOD_DESC char(50),
START_DT date,
FINISH_DT date,
INSERT_DT date,
UPDATE_DT date,
constraint PK_DW_GEN_PERIODS primary key (PERIOD_ID) );


create table SAL_DATA.DIM_BIKE_SCD (
BIKE_ID number,
BRAND_NAME char(20),
MODEL_NAME char(20),
SPEC_NAME char(20),
SPEC_ID number,
BIKE_PRICE number,
SERIAL_NUM number,
VALID_FROM date,
VALID_TO date,
IS_ACTIVE number,
INSERT_DT date,
constraint PK_DW_BIKE_SCD primary key (BIKE_ID) );


create table SAL_DATA.SALE_FACT_TABLE (
SALE_ID number,
EVENT_DT date,
EMPLOYEE_ID number,
BIKE_ID number,
DATE_ID number,
LOCATION_ID number,
PERIOD_ID number,
CUSTOMER_ID number,
SALE_DCT_QUANTITY number,
SALE_FCT_TOTAL_PRICE number,
INSERT_DT date,
UPDATE_DT date,
constraint PK_DW_SALE_FACT_TABLE primary key (SALE_ID) )
PARTITION BY RANGE (EVENT_DT) SUBPARTITION BY HASH (SALE_ID) SUBPARTITIONS 4 
(
PARTITION YEAR_19 VALUES LESS THAN (TO_DATE('01/01/2020','dd/mm/yyyy')),
PARTITION YEAR_20 VALUES LESS THAN (TO_DATE('01/01/2021','dd/mm/yyyy')),
PARTITION YEAR_21 VALUES LESS THAN (TO_DATE('01/01/2022','dd/mm/yyyy'))
);


alter table SAL_DATA.SALE_FACT_TABLE add constraint FK_SALE_FACT_TABLE_REF_DIM_EMPLOYEE_SCD foreign key (EMPLOYEE_ID)
references SAL_DATA.DIM_EMPLOYEE_SCD (EMPLOYEE_ID) on delete cascade;


alter table SAL_DATA.SALE_FACT_TABLE add constraint FK_SALE_FACT_TABLE_REF_DIM_DATE foreign key (DATE_ID)
references SAL_DATA.DIM_DATE (DATE_ID) on delete cascade;


alter table SAL_DATA.SALE_FACT_TABLE add constraint FK_SALE_FACT_TABLE_REF_DIM_LOCATION foreign key (LOCATION_ID)
references SAL_DATA.DIM_LOCATION (LOCATION_ID) on delete cascade;


alter table SAL_DATA.SALE_FACT_TABLE add constraint FK_SALE_FACT_TABLE_REF_DIM_CUSTOMER foreign key (CUSTOMER_ID)
references SAL_DATA.DIM_CUSTOMER (CUSTOMER_ID) on delete cascade;



alter table SAL_DATA.SALE_FACT_TABLE add constraint FK_SALE_FACT_TABLE_REF_DIM_GEN_PERIODS foreign key (PERIOD_ID)
references SAL_DATA.DIM_GEN_PERIODS (PERIOD_ID) on delete cascade;



alter table SAL_DATA.SALE_FACT_TABLE add constraint FK_SALE_FACT_TABLE_REF_DIM_BIKE_SCD foreign key (BIKE_ID)
references SAL_DATA.DIM_BIKE_SCD (BIKE_ID) on delete cascade;
